					vars.put(&quot;go&quot;,&quot;no&quot;)	
					log.info(ww.getName()+&quot; exist@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@&quot;)

				}else{
					vars.put(&quot;go&quot;,&quot;yes&quot;)	
				}


		     }else{
//			 	log.info(&quot;linux&quot;);
//			 	file=new File(&quot;/usr/&quot;+ vars.get(&quot;path&quot;)+&quot;.txt&quot;);
